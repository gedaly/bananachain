{
"manifest_version": 2,
    "name": "BananaChain",
    "description": "Replaces all mentions of blockchain with banana. It's funnier, and an excellent source of potassium.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],

     "icons":
         { "16": "bananachain-16.png", "128": "bananachain-128.png" }
}
